<mapping>


	<class name="org.akaza.openclinica.web.pform.dto.HtmlDTO">
        <map-to xml="html" ns-uri="http://www.w3.org/1999/xhtml" ns-prefix="h" />
        <!-- map-to xml="html" ns-uri="http://www.w3.org/2002/xforms" /-->
		<field name="head" type="org.akaza.openclinica.web.pform.dto.HeadDTO">
			<bind-xml name="head" node="element" />
		</field>
		<field name="body" type="org.akaza.openclinica.web.pform.dto.BodyDTO">
			<bind-xml name="body" node="element" />
		</field>
	</class>
	<class name="org.akaza.openclinica.web.pform.dto.HeadDTO">
		<map-to xml="head" ns-uri="http://www.w3.org/1999/xhtml" ns-prefix="h"  />
		<field name="title" type="string">
            <bind-xml name="h:title" node="element" xmlns:h="http://www.w3.org/1999/xhtml" />
		</field>
        <field name="model" type="org.akaza.openclinica.web.pform.dto.ModelDTO">
            <bind-xml name="model" node="element" xmlns="http://www.w3.org/2002/xforms" />
        </field>
	</class>
	<class name="org.akaza.openclinica.web.pform.dto.ModelDTO">
		<map-to xml="model" />
		<field name="instance" type="string">
			<bind-xml name="instance" node="element" xmlns="http://www.w3.org/2002/xforms" />
		</field>
		<field name="bind" type="org.akaza.openclinica.web.pform.dto.BindDTO" collection="arraylist">
			<bind-xml name="bind" node="element" xmlns="http://www.w3.org/2002/xforms" />
		</field>
		<field name="itext" type="org.akaza.openclinica.web.pform.dto.ITextDTO">
			<bind-xml name="itext" node="element" />
		</field>
	</class>
	<class name="org.akaza.openclinica.web.pform.dto.ITextDTO">
		<map-to xml="itext" />
		<field name="translation" type="org.akaza.openclinica.web.pform.dto.TranslationDTO">
			<bind-xml name="translation" node="element" />
		</field>
	</class>
	<class name="org.akaza.openclinica.web.pform.dto.TranslationDTO">
		<map-to xml="translation" />
		<field name="lang" type="string">
			<bind-xml name="lang" node="attribute" />
		</field>
		<field name="text" type="org.akaza.openclinica.web.pform.dto.TextDTO" collection="arraylist">
			<bind-xml name="text" node="element" />
		</field>
	</class>
	<class name="org.akaza.openclinica.web.pform.dto.TextDTO">
		<map-to xml="text" />
		<field name="id" type="string">
			<bind-xml name="id" node="attribute" />
		</field>
		<field name="value" type="string">
			<bind-xml name="value" node="element" />
		</field>
	</class>
	<class name="org.akaza.openclinica.web.pform.dto.BindDTO">
		<map-to xml="bind" />
		<field name="nodeSet" type="string">
			<bind-xml name="nodeset" node="attribute" />
		</field>
		<field name="type" type="string">
